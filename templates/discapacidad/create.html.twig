{% extends 'base.html.twig' %}

{% block title %}Discapacidades{% endblock %}

{% block body %}

	{{parent()}}

	<div class="sectionTitle">
		<h2>DISCAPACIDAD</h2>
	</div>

	{% for message in app.flashes('exito') %}
            <div class="alert alert-success">
                {{ message }}
            </div>
    {% endfor %}
	
	{{form(formulario)}}

	<div class="btnCancelar">
        <a href="/discapacidad" class="btn btn-danger mb-4" id="cancelar">Cancelar</a>
    </div>

{% endblock %}
